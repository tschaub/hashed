{
  "name": "hashed",
  "version": "5.0.0",
  "description": "Serialize state from multiple providers using location.hash",
  "homepage": "https://github.com/tschaub/hashed",
  "author": {
    "name": "Tim Schaub",
    "url": "http://tschaub.net/"
  },
  "keywords": [
    "hash",
